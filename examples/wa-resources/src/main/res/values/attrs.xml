<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <attr name="key" format="string" tools:ignore="ResourceName" />
    <attr name="keyTextSize" format="dimension" tools:ignore="ResourceName" />
    <attr name="keyTextColor" format="color" tools:ignore="ResourceName" />
    <attr name="keyTextGravity" tools:ignore="ResourceName">
        <flag name="start" value="0x00800003" />
        <flag name="end" value="0x00800005" />
    </attr>

    <attr name="value" format="string" tools:ignore="ResourceName" />
    <attr name="valueTextSize" format="dimension" tools:ignore="ResourceName" />
    <attr name="valueTextColor" format="color" tools:ignore="ResourceName" />
    <attr name="valueHintColor" format="color" tools:ignore="ResourceName" />
    <attr name="valueTextHint" format="string" tools:ignore="ResourceName" />
    <attr name="valueTextMinLines" format="integer" tools:ignore="ResourceName" />
    <attr name="valueEditable" format="boolean" tools:ignore="ResourceName" />
    <attr name="valueMultipleEnable" format="boolean" tools:ignore="ResourceName" />
    <attr name="valueInputType" tools:ignore="ResourceName">
        <flag name="none" value="0x00000000" />
        <flag name="text" value="0x00000001" />
        <flag name="textCapCharacters" value="0x00001001" />
